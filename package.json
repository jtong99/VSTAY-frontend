{
  "name": "v-stay-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "analyze": "ANALYZE=true next build",
    "up": "docker-compose up -d",
    "down": "docker-compose down",
    "rebuild": "docker-compose up -d --build",
    "commit": "git add -A && git commit -m",
    "log": "docker logs knowllipop_frontend",
    "into": "docker exec -it knowllipop_frontend /bin/sh",
    "eslint-check": "eslint --print-config pages/index.jsx | eslint-config-prettier-check",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "merge:production": "git checkout production && git pull && git merge master && git push && git checkout master"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-directions": "^4.1.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "eslint-plugin-import": "^2.22.1",
    "express": "^4.17.1",
    "firebase": "^7.24.0",
    "fs": "0.0.1-security",
    "gifsicle": "^3.0.4",
    "helmet": "^4.1.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "js-cookie": "^2.2.1",
    "mapbox-gl": "^1.12.0",
    "next": "9.5.3",
    "next-compose-plugins": "^2.2.0",
    "next-cookies": "^2.0.3",
    "next-i18next": "^4.3.0",
    "next-optimized-images": "^2.6.2",
    "next-seo": "^4.11.0",
    "path": "^0.12.7",
    "react": "16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "16.13.1",
    "react-feather": "^2.0.8",
    "react-map-gl": "^5.2.9",
    "react-map-gl-directions": "^1.0.4",
    "react-mapbox-gl-geocoder": "^1.1.0",
    "sass": "^1.27.0",
    "svg-sprite-loader": "^5.0.0",
    "swr": "^0.3.5",
    "unfetch": "^4.2.0",
    "webp-loader": "^0.6.0",
    "webpack-cli": "^4.0.0",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "babel-eslint": "^10.1.0",
    "commitlint": "^11.0.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2"
  },
  "lint-staged": {
    "**/*.(js|jsx)": [
      "eslint --fix"
    ],
    "**/*.(json|css|md)": [
      "prettier --write"
    ]
  },
  "eslintIgnore": [
    "build/**",
    "packages/**"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
